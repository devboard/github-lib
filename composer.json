{
    "name": "devboard/github-lib",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Devboard\\GitHub\\": "src/",
            "tests\\Devboard\\GitHub\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Devboard\\GitHub\\": "spec/"
        }
    },
    "require": {
        "php":                                      ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit":                          "^5",
        "mockery/mockery":                          "~0.9",
        "phpspec/phpspec":                          "~3.0",
        "henrikbjorn/phpspec-code-coverage":        "^3.0",
        "ciaranmcnulty/phpspec-typehintedmethods":  "~2.0",
        "phpmd/phpmd":                              "~2.1",
        "squizlabs/php_codesniffer":                "^3.0",
        "friendsofphp/php-cs-fixer":                "^2.0",
        "satooshi/php-coveralls":                   "^1",
        "codeclimate/php-test-reporter":            "^0.4.2",
        "sebastian/phpcpd":                         "^3.0",
        "phing/phing":                              "^2.15",
        "phpstan/phpstan":                          "^0.7.0",
        "humbug/humbug":                            "~1.0@dev"
    },
    "config": {
        "platform": {
            "php": "7.1.0"
        },
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    }
}
